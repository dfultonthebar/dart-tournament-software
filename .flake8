[flake8]
max-line-length = 100
extend-ignore = E203, E266, E501, W503
exclude =
    .git,
    __pycache__,
    venv,
    env,
    .venv,
    build,
    dist,
    .eggs,
    *.egg-info,
    .tox,
    .mypy_cache,
    .pytest_cache
max-complexity = 10
per-file-ignores =
    __init__.py:F401
    */migrations/*:E501
